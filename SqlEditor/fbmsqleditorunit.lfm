  object quFindQuery: TZMacroQuery
    Connection = UserDBModule.UserDB
    AfterOpen = quFindQueryAfterScroll
    AfterScroll = quFindQueryAfterScroll
    SQL.Strings = (
      'select'
      '  sql_editors.sql_editors_id,'
      '  sql_editors.sql_editors_caption,'
      '  sql_editors.sql_editors_body,'
      '  db_database.db_database_id,'
      '  db_database.db_database_sql_engine,'
      '  db_database.db_database_caption,'
      '  (select'
      '     cast(max(sql_editors_history.sql_editors_history_date) as varchar(25))'
      '   from'
      '     sql_editors_history'
      '   where'
      '       sql_editors_history.db_database_id = db_database.db_database_id'
      '     and'
      '       sql_editors_history.sql_editors_history_sql_page_name = sql_editors.sql_editors_caption) as sql_editors_history_date'
      'from'
      '  sql_editors'
      '  inner join db_database on (sql_editors.db_database_id = db_database.db_database_id)'
      'where'
      '  %Macro1'
      'order by'
