      '  db_database.db_database_id,'
      '  db_database.db_database_sql_engine,'
      '  db_database.db_database_caption,'
      '  (select'
      '     cast(max(sql_editors_history.sql_editors_history_date) as varchar(25))'
      '   from'
      '     sql_editors_history'
      '   where'
      '       sql_editors_history.db_database_id = db_database.db_database_id'
