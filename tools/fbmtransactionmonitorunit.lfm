    SQLSelect.Strings = (
      'SELECT'
      '  mon$attachments.MON$ATTACHMENT_ID,'
      '  mon$attachments.MON$STATE,'
      '  mon$attachments.MON$ATTACHMENT_NAME,'
      '  mon$attachments.MON$USER,'
      '  mon$attachments.MON$ROLE,'
      '  mon$attachments.MON$REMOTE_PROTOCOL,'
      '  mon$attachments.MON$REMOTE_ADDRESS,'
      '  mon$attachments.MON$REMOTE_PROCESS,'
      '  mon$attachments.MON$TIMESTAMP,'
      '  rdb$character_sets.RDB$CHARACTER_SET_NAME,'
      '  mon$record_stats.*,'
      '  mon$io_stats.*'
      'from'
      '  mon$attachments'
      '  inner join rdb$character_sets on (mon$attachments.mon$character_set_id = rdb$character_sets.rdb$character_set_id)'
      '  left join mon$record_stats on (mon$attachments.mon$stat_id = mon$record_stats.mon$stat_id)'
